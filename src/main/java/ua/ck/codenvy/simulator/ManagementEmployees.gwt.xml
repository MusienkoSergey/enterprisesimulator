<module rename-to='ManagementEmployees'>

    <!-- Inherit the core Web Toolkit stuff. -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="org.fusesource.restygwt.RestyGWT"/>

    <!-- Specify the GeneratorEmployees entry point class. -->
    <entry-point class='ua.ck.codenvy.simulator.client.ManagementEmployees'/>

    <!-- Specify the paths for translatable code -->
    <source path='client'/>
    <source path='shared'/>

